{
	"extends": [
		"react-app",
		"react-app/jest"
	],
	"rules": {
		"no-unused-vars": "off",
		"quotes": [
			"error",
			"single"
		],
		"jsx-quotes": [
			"error",
			"prefer-double"
		],
		"prefer-const": "warn",
		"indent": [
			"warn",
			2
		],
		"max-len": [
			"warn",
			{
				"code": 120
			}
		],
		"comma-dangle": [
			"warn",
			"always-multiline"
		],
		"semi": [
			"warn",
			"always"
		],
		"import/order": [
			"error",
			{
				"newlines-between": "always-and-inside-groups"
			}
		]
	}
}